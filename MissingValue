function MissingValue(arr){
  let total=0
  for(let i=0;i<=7;i++){
    total+=i
  }
  let sum=0;
  for(let i=0;i<arr.length;i++){
    sum+=arr[i]
  }
  console.log(total-sum)
  
}
console.log(MissingValue([1,2,3,5,6,7]))
//missing two values

function findMissing(arr, n) {
  let set = new Set(arr);
  let missing = [];

  for (let i = 1; i <= n; i++) {
    if (!set.has(i)) {
      missing.push(i);
    }
  }

  return missing;
}

console.log(findMissing([1,2,3,6,7], 7));
